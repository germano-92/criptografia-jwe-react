{
  "compilerOptions": {
    "noEmit": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": { "@utils/*": ["utils/*"] },
    //"module": "NodeNext",
    //"moduleResolution": "NodeNext",
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react"
  },
  "include": ["src"]
}
